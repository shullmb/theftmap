<div class="alert row">
    <% if (alerts.error) { %>
        <% alerts.error[0].split(',').forEach( function(msg)  { %>
            <div class="flash col s8 offset-s2 deep-orange lighten-3"><%= msg %></div>
        <% }) %>
    <% } %>

    <% if (alerts.success) { %>
        <% alerts.success[0].split(',').forEach( function(msg)  { %>
            <div class="flash col s8 offset-s2 light-green lighten-3"><%= msg %></div>
        <% }) %>
    <% } %>
</div>
